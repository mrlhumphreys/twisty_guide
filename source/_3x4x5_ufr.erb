<polygon points="80,20 180,70 180,150 120,180 20,130 20,50" style="fill:<%= data.colours.fill %>;stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- x axis -->
<!-- up face -->
<line x1="20" y1="50" x2="120" y2="100" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="40" y1="40" x2="140" y2="90" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="60" y1="30" x2="160" y2="80" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- front face -->
<line x1="20" y1="70" x2="120" y2="120" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="20" y1="90" x2="120" y2="140" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="20" y1="110" x2="120" y2="160" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- y axis -->
<!-- front face -->
<line x1="40" y1="60" x2="40" y2="140" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="60" y1="70" x2="60" y2="150" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="80" y1="80" x2="80" y2="160" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="100" y1="90" x2="100" y2="170" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="120" y1="100" x2="120" y2="180" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- right face -->
<line x1="140" y1="90" x2="140" y2="170" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="160" y1="80" x2="160" y2="160" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- z axis -->
<!-- up face -->
<line x1="40" y1="60" x2="100" y2="30" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="60" y1="70" x2="120" y2="40" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="80" y1="80" x2="140" y2="50" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="100" y1="90" x2="160" y2="60" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="120" y1="100" x2="180" y2="70" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />

<!-- right face -->
<line x1="120" y1="120" x2="180" y2="90" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="120" y1="140" x2="180" y2="110" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />
<line x1="120" y1="160" x2="180" y2="130" style="stroke:<%= data.colours.stroke %>;stroke-width:1" />